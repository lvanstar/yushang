<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/homepageback">
    <RelativeLayout
        style="@style/Title"
        android:background="@color/titlecolor3">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="我的寓上"
            android:textColor="@color/defaulttxt2"
            android:textSize="@dimen/text_large" />
        <RelativeLayout
            android:id="@+id/my_settings"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true">
            <ImageView
                android:layout_width="23dp"
                android:layout_height="23dp"
                android:layout_centerInParent="true"
                android:scaleType="fitCenter"
                android:src="@drawable/setting" />
        </RelativeLayout>
    </RelativeLayout>
    <View
        android:layout_width="fill_parent"
        android:layout_height="@dimen/xian"
        android:background="@color/xian"></View>
    <enjoytouch.com.redstar.selfview.PushView
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="1">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <View
                android:layout_width="fill_parent"
                android:layout_height="10dp"></View>
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="70dp"
                android:background="@color/white"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/iv_portrait"
                    android:layout_width="80dp"
                    android:layout_height="49dp"
                    android:layout_gravity="center"
                    android:src="@drawable/morentouxiang"
                    />
                <TextView
                    android:id="@+id/tv_phonenumber"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:textColor="@color/text01"
                    android:textSize="@dimen/text_middle"
                    android:layout_gravity="center_vertical"
                    android:gravity="center_vertical"
                    android:text="未登录"/>
                <View
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"></View>
                <TextView
                    android:id="@+id/tv_replacenumber"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:textColor="@color/lan"
                    android:textSize="@dimen/text_middle"
                    android:layout_gravity="center_vertical"
                    android:gravity="center_vertical"
                    android:layout_marginRight="15dp"
                    android:text="更换手机号"/>

            </LinearLayout>
            <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/xian"
                android:background="@color/xian"></View>
            <View
                android:layout_width="fill_parent"
                android:layout_height="8dp"></View>
            <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/xian"
                android:background="@color/xian"></View>
            <LinearLayout
                android:id="@+id/ll_tomyorder"
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:background="@color/white"
                android:orientation="horizontal">
                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:src="@drawable/myorder"
                    android:layout_marginLeft="10dp"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:textColor="@color/text01"
                    android:textSize="@dimen/text_middle"
                    android:layout_gravity="center_vertical"
                    android:gravity="center_vertical"
                    android:text="  我的订单"/>
                <View
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"></View>
                <TextView
                    android:id="@+id/tv_ordernumber"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:textColor="@color/text01"
                    android:textSize="@dimen/text_middle"
                    android:layout_gravity="center_vertical"
                    android:gravity="center_vertical"
                    />
                <View
                    android:layout_width="5dp"
                    android:layout_height="fill_parent"></View>
                <ImageView
                    android:layout_width="10dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:background="@drawable/youjiantouhui"
                    android:layout_marginRight="10dp"/>
            </LinearLayout>
            <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/xian"
                android:layout_marginLeft="10dp"
                android:background="@color/xian"></View>
            <LinearLayout
                android:id="@+id/tomyhot"
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:background="@color/white"
                android:onClick="toMyHotStyle"
                android:orientation="horizontal">
                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:src="@drawable/myhot"
                    android:layout_marginLeft="10dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:textColor="@color/text01"
                    android:textSize="@dimen/text_middle"
                    android:layout_gravity="center_vertical"
                    android:gravity="center_vertical"
                    android:text="  我收藏的爆款"/>
                <View
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"></View>
                <TextView
                    android:id="@+id/tv_hotnumber"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:textColor="@color/text01"
                    android:textSize="@dimen/text_middle"
                    android:layout_gravity="center_vertical"
                    android:gravity="center_vertical"
                    />
                <View
                    android:layout_width="5dp"
                    android:layout_height="fill_parent"></View>
                <ImageView
                    android:layout_width="10dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:background="@drawable/youjiantouhui"
                    android:layout_marginRight="10dp"/>
            </LinearLayout>
            <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/xian"
                android:background="@color/xian"></View>
            <View
                android:layout_width="fill_parent"
                android:layout_height="8dp"></View>
            <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/xian"
                android:background="@color/xian"></View>
            <LinearLayout
                android:id="@+id/tomycoupon"
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:background="@color/white"
                android:orientation="horizontal">
                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:src="@drawable/myconpon"
                    android:layout_marginLeft="10dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:textColor="@color/text01"
                    android:textSize="@dimen/text_middle"
                    android:layout_gravity="center_vertical"
                    android:gravity="center_vertical"
                    android:text="  我的优惠券"/>
                <View
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"></View>
                <TextView
                    android:id="@+id/tv_couponsnumber"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:textColor="@color/text01"
                    android:textSize="@dimen/text_middle"
                    android:layout_gravity="center_vertical"
                    android:gravity="center_vertical"
                    />
                <View
                    android:layout_width="5dp"
                    android:layout_height="fill_parent"></View>
                <ImageView
                    android:layout_width="10dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:background="@drawable/youjiantouhui"
                    android:layout_marginRight="10dp"/>
            </LinearLayout>
            <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/xian"
                android:layout_marginLeft="10dp"
                android:background="@color/xian"></View>
            <LinearLayout
                android:id="@+id/tomycelect"
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:background="@color/white"
                android:orientation="horizontal">
                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:src="@drawable/loveconpon"
                    android:layout_marginLeft="10dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:textColor="@color/text01"
                    android:textSize="@dimen/text_middle"
                    android:layout_gravity="center_vertical"
                    android:gravity="center_vertical"
                    android:text="  我收藏的优惠券"/>
                <View
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"></View>
                <TextView
                    android:id="@+id/tv_mycouponsnumber"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:textColor="@color/text01"
                    android:textSize="@dimen/text_middle"
                    android:layout_gravity="center_vertical"
                    android:gravity="center_vertical"
                    />
                <View
                    android:layout_width="5dp"
                    android:layout_height="fill_parent"></View>
                <ImageView
                    android:layout_width="10dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:background="@drawable/youjiantouhui"
                    android:layout_marginRight="10dp"/>
            </LinearLayout>
            <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/xian"
                android:background="@color/xian"></View>
            <View
                android:layout_width="fill_parent"
                android:layout_height="8dp"></View>
            <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/xian"
                android:background="@color/xian"></View>
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:onClick="toMyBrand"
                android:background="@color/white"
                android:orientation="horizontal">
                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:src="@drawable/mybrand"
                    android:layout_marginLeft="10dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:textColor="@color/text01"
                    android:textSize="@dimen/text_middle"
                    android:layout_gravity="center_vertical"
                    android:gravity="center_vertical"
                    android:text="  我关注的品牌"/>
                <View
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"></View>
                <TextView
                    android:id="@+id/tv_mybrandnumber"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:textColor="@color/text01"
                    android:textSize="@dimen/text_middle"
                    android:layout_gravity="center_vertical"
                    android:gravity="center_vertical"
                    />
                <View
                    android:layout_width="5dp"
                    android:layout_height="fill_parent"></View>
                <ImageView
                    android:layout_width="10dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:background="@drawable/youjiantouhui"
                    android:layout_marginRight="10dp"/>
            </LinearLayout>
            <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/xian"
                android:layout_marginLeft="10dp"
                android:background="@color/xian"></View>
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:onClick="toMyStore"
                android:background="@color/white"
                android:orientation="horizontal">
                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:src="@drawable/mystore"
                    android:layout_marginLeft="10dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:textColor="@color/text01"
                    android:textSize="@dimen/text_middle"
                    android:layout_gravity="center_vertical"
                    android:gravity="center_vertical"
                    android:text="  我关注的店铺"/>
                <View
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"></View>
                <TextView
                    android:id="@+id/tv_mystorenumber"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:textColor="@color/text01"
                    android:textSize="@dimen/text_middle"
                    android:layout_gravity="center_vertical"
                    android:gravity="center_vertical"
                    />
                <View
                    android:layout_width="5dp"
                    android:layout_height="fill_parent"></View>
                <ImageView
                    android:layout_width="10dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:background="@drawable/youjiantouhui"
                    android:layout_marginRight="10dp"/>
            </LinearLayout>
            <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/xian"
                android:background="@color/xian"></View>
            <View
                android:layout_width="fill_parent"
                android:layout_height="8dp"></View>
            <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/xian"
                android:background="@color/xian"></View>
            <LinearLayout
                android:id="@+id/tomyaddress"
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:background="@color/white"
                android:orientation="horizontal">
                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:src="@drawable/myaddress"
                    android:layout_marginLeft="10dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:textColor="@color/text01"
                    android:textSize="@dimen/text_middle"
                    android:layout_gravity="center_vertical"
                    android:gravity="center_vertical"
                    android:text="  我的收货地址"/>
                <View
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"></View>

                <ImageView
                    android:layout_width="10dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:background="@drawable/youjiantouhui"
                    android:layout_marginRight="10dp"/>
            </LinearLayout>
            <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/xian"
                android:background="@color/xian"></View>

        </LinearLayout>

    </enjoytouch.com.redstar.selfview.PushView>


</LinearLayout>