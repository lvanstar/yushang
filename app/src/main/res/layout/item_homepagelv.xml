<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fresco="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.facebook.drawee.view.SimpleDraweeView
            android:id="@+id/iv10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            fresco:placeholderImage="@drawable/placeholder_rectangle"
            fresco:placeholderImageScaleType="centerCrop"/>
        <ImageView
            android:id="@+id/homepage_iv01"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@drawable/home_mask"
            android:scaleType="fitXY"/>
        <RelativeLayout
            android:id="@+id/alpha"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        </RelativeLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="40px"
            android:layout_marginTop="40px"
            android:orientation="vertical">
            <View
                android:id="@+id/homepage_01"
                android:layout_width="match_parent"
                android:layout_height="@dimen/xian2"
                android:background="@color/white"
                android:visibility="gone"
                />
            <TextView
                android:id="@+id/iv10_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/text_middle"
                android:includeFontPadding="false"
                android:textColor="@color/white"
                android:gravity="top|left"
                android:text=""/>
            <TextView
                android:id="@+id/iv10_content"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/text_small"
                android:includeFontPadding="false"
                android:textColor="@color/white"
                android:layout_marginTop="4px"
                android:text=""/>
            <View
                android:id="@+id/homepage_02"
                android:layout_width="match_parent"
                android:layout_height="@dimen/xian2"
                android:background="@color/white"
                android:layout_marginTop="4px"
                android:visibility="gone"/>
        </LinearLayout>
    </RelativeLayout>
</LinearLayout>