<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/homepageback">
    <ImageView
        android:id="@+id/notuse_pic"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:src="@drawable/a24"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="130dp"
        android:visibility="gone"/>

    <TextView
        android:id="@+id/notuse_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/notuse_pic"
        android:layout_marginTop="15dp"
        android:text="亲~木有未使用的优惠券咩~"
        android:textSize="18sp"
        android:textColor="#9e9e9e"
        android:layout_centerHorizontal="true"
        android:visibility="gone"/>

    <!--<enjoytouch.com.redstar.selfview.HorizontalSlideListView-->
        <!--android:listSelector="#00000000"-->
        <!--android:id="@+id/notused_lv"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="match_parent"-->
        <!--android:dividerHeight="4dp"-->
        <!--android:layout_marginLeft="12dp"-->
        <!--android:layout_marginRight="12dp"/>-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1162">
    <enjoytouch.com.redstar.selfview.swipemenulistview.SwipeMenuListView
        android:id="@+id/notused_lv"
        android:layout_width="match_parent"
        android:divider="#00000000"
        android:dividerHeight="10dp"
        android:layout_height="match_parent"
        android:layout_marginLeft="6dp"
        android:layout_marginRight="6dp"
        android:listSelector="#00000000"/>
    </LinearLayout>
    </LinearLayout>
</RelativeLayout>
